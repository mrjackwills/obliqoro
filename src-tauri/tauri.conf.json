{
  "$schema": "https://schema.tauri.app/config/2",
	"build": {
		"beforeBuildCommand": "npm run build",
		"beforeDevCommand": "npm run dev",
		"frontendDist": "../dist",
		"devUrl": "http://localhost:9000"
	},
	"bundle": {
		"active": true,
		"category": "Productivity",
		"copyright": "",
		"targets": [
			"deb",
			"msi",
			"app",
			"dmg",
			"rpm"
		],
		"externalBin": [],
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"windows": {
			"certificateThumbprint": null,
			"digestAlgorithm": "sha256",
			"timestampUrl": "",
			"webviewInstallMode": {
				"type": "embedBootstrapper"
			}
		},
		"longDescription": "Obliqoro is an open source, cross platform, graphical application, that combines the creative approach of Oblique Strategies with the time management system of the Pomodoro Technique.",
		"macOS": {
			"entitlements": null,
			"exceptionDomain": "",
			"frameworks": [],
			"providerShortName": null,
			"signingIdentity": null
		},
		"resources": [],
		"shortDescription": "Oblique Strategies meets Pomodoro",
		"linux": {
			"deb": {
				"depends": []
			}
		},
		"createUpdaterArtifacts": false
	},
	"productName": "Obliqoro",
	"mainBinaryName": "Obliqoro",
	"version": "0.3.1",
	"identifier": "mrjackwills.obliqoro",
	"plugins": {},
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"alwaysOnTop": true,
				"center": true,
				"decorations": false,
				"fullscreen": false,
				"height": 850,
				"label": "main",
				"resizable": false,
				"shadow": false,
				"skipTaskbar": true,
				"title": "Obliqoro",
				"useHttpsScheme": true,
				"visible": false,
				"width": 1000
			}
		],
		"security": {
			"csp": null
		}
	}
}